import java.util.*;
import java.io.*;

class hallowsquare
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int row,i,j,col;
System.out.println("enter the number of rows and col");
row=sc.nextInt();
col=sc.nextInt();
for(i=1;i<=row;i++)
{
for(j=1;j<=col;j++)
if((i==1 || i==row ||j==1 || j==col))
System.out.print("*");
else
System.out.print (" ");
System.out.println(" ");

}
}
}
