import java.util.*;
import java.io.*;
class fact
{
public static void main(String args[])
{
try
{
Scanner sc = new Scanner(System.in);
int i,fact=1;
System.out.println("enter the n value");
int n=sc.nextInt();
for( i=1;i<=n;i++)
{
 fact=fact*i; 

System.out.println(fact);
 }
}
catch(Exception e)
{
System.out.println("invalid input");
}
}
}
