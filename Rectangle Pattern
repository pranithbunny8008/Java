import java.util.*;
import java.io.*;

class rectpattern
{
public static void main(String args[])
{
Scanner sc = new Scanner(System.in);
int row,col,i,j;
System.out.println("enter the number of rows");
row=sc.nextInt();
System.out.println("enter the number of col");
col=sc.nextInt();
for(i=1;i<=row;i++)

{
System.out.println(" ");

for(j=1;j<=col;j++)
{
System.out.print("*");
}
}
}
}
